.Table
  border-collapse collapse
  width 95%
  margin 0 auto 2em auto


.Table-rowBody:nth-child(even) {background-color #f2f2f2}

.Table, .Table-itemHeader, .Table-itemBody
  border 1px solid white


.Table-itemHeader
  background-color #4393B9
  color white
  height 1em


.Table-itemHeader
   padding 15px
   text-align left

.Table-itemBody
   padding 15px
   text-align left
