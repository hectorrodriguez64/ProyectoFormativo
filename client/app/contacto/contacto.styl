.Contacto
   padding 0 8px
.Padding64
   padding-top 15px
   padding-bottom 64px
.Contacto-span
   padding-top 16px
   padding-bottom 16px
   font-size 24px
   color #2b2525
.Contacto-marginBottom
   margin-bottom 16px
.Contacto-card
   position relative
   display -ms-flexbox
   display -webkit-box
   display -webkit-flex
   display flex
   -webkit-box-orient vertical
   -webkit-box-direction normal
   -webkit-flex-direction column
   flex-direction column
   word-wrap break-word
   background-color #fff
   background-clip border-box
   border 1px solid rgba(0,0,0,.125);
   font-weight 400
   border-radius 1.3rem
   box-shadow 0 4px 10px 0 rgba(0,0,0,0.2), 0 4px 20px 0 rgba(0,0,0,0.19)
.Card-img
   overflow visible
   box-shadow 0 8px 17px 0 rgba(0,0,0,.2), 0 6px 20px 0 rgba(0,0,0,.19)
   cursor pointer
.Img-Card
   border-radius 70%
   height 20em
   width 58%
   margin auto
   transition all .2s linear
.Card-mask
   height 100%
   position absolute
   overflow hidden
   top 0
   left 0
   background-color rgba(255,255,255,.1)
   opacity 0
   -webkit-transition all .4s ease-in-out
   transition all .4s ease-in-out
.Card-body
   padding-top 1.8rem
   -webkit-box-flex 1
   -webkit-flex 1 1 auto
   flex 1 1 auto
   padding 1.25rem
.Card-title
   line-height: 1.1
   font-weight: 400
.Card-text
   margin-bottom: 1rem
.Contacto-container
   padding 0.01em 16px
.Contacto-opacity
   opacity 0.60
.Contacto-button
   color #000 !important
   background-color #f1f1f1 !important
   width 100%
   border none
   outline 0
   padding 8px 16px
   text-decoration none
   cursor pointer
.Contacto-button:hover
   color #000 !important
   background-color gray !important
.Contacto-padding64
   padding-top 64px
   padding-bottom 64px
.Contacto-padding16
   padding-top 16px
   padding-bottom 16px
.Contacto-spantTitle
   border-bottom 6px solid #ccc
   font-size 24px
.Contacto-icons
   color #009688
   font-size 24px !important
.Social-icon
   -webkit-appearance none !important
.Icon-face
   font-size 15px !important
   line-height 31px !important
   text-align center
   transition all 0.5s ease-in-out !important
   -webkit-transition all 0.5s ease-in-out
   -moz-transition all 0.5s ease-in-out
   -o-transition all 0.5s ease-in-out
   -ms-transition all 0.5s ease-in-out
   width 36px
   height 36px
   border 2px solid
   border-radius 4em
   box-shadow 0 4px 10px 0 rgba(0,0,0,0.2), 0 4px 20px 0 rgba(0,0,0,0.19)
   cursor pointer
   
 /*-- contact --*/  
.Contact-iconTitle
    font-weight 600
    font-size 1.2em
    padding-bottom .5em
    position relative
    margin-bottom 1em

.Contact-iconSpan
    padding-right 1em
    color #428bca
    font-size 1.4em

.Contact-iconDesc
    color #202020
    line-height 2em
    font-size 15px
    letter-spacing 2px

.Contact-info_map_color_grid
    padding 3em
    text-align center

.Contact-iconEmail
    color #202020
    text-decoration none !important
    &:hover
        color #23D6E7

.Conect-socialIcon
    display inline-block

.Connect-social
    ul
        li
            a
                display inline-block
                width 35px
                height 35px
                color #585858
                border 1px solid #585858
                line-height 2.4em
                border-radius 50%
                text-align center

a
    &.Social-button
        &.facebook
            &:hover
                background #3b5998
                color #fff
                border 1px solid #fff
        &.twitter
            &:hover
                background #1da1f2
                color #fff
                border 1px solid #fff
        &.google
            &:hover
                background #dd4b39
                color #fff
                border 1px solid #fff
/* //contact */
